{
  "name": "api-swoole/sapi",
  "description": "基于swoole实现的一个轻量级框架，支持http、websocket、tcp、udp。",
  "type": "library",
  "keywords": [
    "php",
    "swoole",
    "api-swoole",
    "coroutine",
    "tcp",
    "udp",
    "websocket"
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=7.4",
    "ext-swoole": ">=4.5"
  },
  "autoload": {
    "psr-4": {
      "Sapi\\": "src/",
      "Co\\": "Co/"
    },
    "files": [
      "src/function.php",
      "src/bootstrap.php",
      "Co/function.php"
    ]
  }
}
